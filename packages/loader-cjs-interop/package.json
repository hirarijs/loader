{
  "name": "@hirarijs/loader-cjs-interop",
  "version": "1.0.19",
  "description": "CJS interop loader plugin that exposes named exports for CommonJS modules",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --target es2020 --tsconfig tsconfig.json",
    "test": "node --test"
  },
  "peerDependencies": {
    "@hirarijs/loader": "*"
  },
  "devDependencies": {
    "@hirarijs/loader": "workspace:*",
    "tsup": "^8.0.1"
  },
  "dependencies": {}
}
